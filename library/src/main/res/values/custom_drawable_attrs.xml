        <attr name="shape2GradientStartColor" format="color" />
        <attr name="shape2GradientCenterColor" format="color" />
        <attr name="shape2GradientEndColor" format="color" />
        <attr name="shape2GradientRadius" format="dimension" />

    </declare-styleable>
    <!--shape2结束-->

    <!--selector-->
    <declare-styleable name="Custom_Selector">
        <attr name="selectorStateActive" format="reference|color|enum">
            <enum name="shape" value="0" />
            <enum name="shape1" value="1" />
            <enum name="shape2" value="2" />
        </attr>
        <attr name="selectorStateActivated" format="reference|color|enum">
            <enum name="shape" value="0" />
            <enum name="shape1" value="1" />
            <enum name="shape2" value="2" />
        </attr>
        <attr name="selectorStateChecked" format="reference|color|enum">
            <enum name="shape" value="0" />
            <enum name="shape1" value="1" />
            <enum name="shape2" value="2" />
        </attr>
        <attr name="selectorStateEnabled" format="reference|color|enum">
            <enum name="shape" value="0" />
            <enum name="shape1" value="1" />
            <enum name="shape2" value="2" />
        </attr>
        <attr name="selectorStateFocused" format="reference|color|enum">
            <enum name="shape" value="0" />
            <enum name="shape1" value="1" />
            <enum name="shape2" value="2" />
        </attr>
        <attr name="selectorStatePressed" format="reference|color|enum">
            <enum name="shape" value="0" />
            <enum name="shape1" value="1" />
            <enum name="shape2" value="2" />
        </attr>
        <attr name="selectorStateNormal" format="reference|color|enum">
            <enum name="shape" value="0" />
            <enum name="shape1" value="1" />
            <enum name="shape2" value="2" />
        </attr>
        <!--selector结束-->
    </declare-styleable>

    <declare-styleable name="CustomDrawable">
        <attr name="setAs" format="enum">
            <enum name="background" value="0" />
            <enum name="src" value="1" />
            <enum name="foreground" value="2" />
        </attr>
        <attr name="drawableType" format="enum">
            <enum name="shape" value="0" />
            <enum name="selector" value="1" />
        </attr>
    </declare-styleable>

</resources>