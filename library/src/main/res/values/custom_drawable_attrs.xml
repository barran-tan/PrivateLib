        <attr name="shape1SolidColor" format="color" />

        <attr name="shape1StrokeWidth" format="dimension" />
        <attr name="shape1StrokeColor" format="color" />

        <attr name="shape1CornerRadius" format="dimension" />
        <attr name="shape1CornerRadiusTopLeft" format="dimension" />
        <attr name="shape1CornerRadiusTopRight" format="dimension" />
        <attr name="shape1CornerRadiusBottomLeft" format="dimension" />
        <attr name="shape1CornerRadiusBottomRight" format="dimension" />

        <attr name="shape1GradientType" format="enum">
            <enum name="linear" value="0" />
            <enum name="radial" value="1" />
            <enum name="sweep" value="2" />
        </attr>
        <attr name="shape1GradientAngle" format="enum">
            <enum name="t2b" value="0" />
            <enum name="tr2bl" value="1" />
            <enum name="r2l" value="2" />
            <enum name="br2tl" value="3" />
            <enum name="b2t" value="4" />
            <enum name="bl2tr" value="5" />
            <enum name="l2r" value="6" />
            <enum name="tl2br" value="7" />

        </attr>
        <attr name="shape1GradientStartColor" format="color" />
        <attr name="shape1GradientCenterColor" format="color" />
        <attr name="shape1GradientEndColor" format="color" />
        <attr name="shape1GradientRadius" format="dimension" />

    </declare-styleable>
    <!--shape1结束-->

    <!--shape2-->
    <declare-styleable name="Custom_Shape2">
        <attr name="shape2Type" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="oval" value="1" />
            <enum name="line" value="2" />
            <enum name="ring" value="3" />
        </attr>

        <attr name="shape2SolidSize" format="dimension" />
        <attr name="shape2SolidWidth" format="dimension" />
        <attr name="shape2SolidHeight" format="dimension" />
        <attr name="shape2SolidColor" format="color" />

        <attr name="shape2StrokeWidth" format="dimension" />
        <attr name="shape2StrokeColor" format="color" />

        <attr name="shape2CornerRadius" format="dimension" />
        <attr name="shape2CornerRadiusTopLeft" format="dimension" />
        <attr name="shape2CornerRadiusTopRight" format="dimension" />
        <attr name="shape2CornerRadiusBottomLeft" format="dimension" />
        <attr name="shape2CornerRadiusBottomRight" format="dimension" />

        <attr name="shape2GradientType" format="enum">
            <enum name="linear" value="0" />
            <enum name="radial" value="1" />
            <enum name="sweep" value="2" />
        </attr>
        <attr name="shape2GradientAngle" format="enum">
            <enum name="t2b" value="0" />
